<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="batch">
    
    <select id="getCycleList" resultType="cycleVo">
        select *
          from cycle
        order by cid, pid
    </select>
</mapper>